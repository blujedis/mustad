{
  "name": "mustad",
  "version": "0.1.8",
  "description": "Pre and post hooks for functions such as node, mongodb etc.",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "watch": "tsc -w",
    "start": "npm run watch",
    "typedoc": "typedoc ./src --options ./typedoc.json --out ./docs",
    "docs": "npm run typedoc && touch ./docs/.nojekll",
    "test": "mocha test/**/*.spec.ts* --require ./register.js --bail",
    "prep": "npm run build && npm run docs && npm run test"
  },
  "files": [
    "fixtures",
    "README.md",
    "dist"
  ],
  "keywords": [
    "hooks",
    "kareem",
    "promise-hooks",
    "node hooks",
    "before-after-hooks",
    "mustad"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.22",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.6"
  }
}
